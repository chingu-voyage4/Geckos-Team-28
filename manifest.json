{
  "manifest_version": 2,

  "name": "Chingu Medium Digest",
  "description": "This page is will display all recent articles from pre-selected Chingu members.",
  "version": "0.0.0.1",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": ["scripts/background.js"]
  },

  "permissions": [
    "https://www.jasonbase.com/*",
    "activeTab",
    "storage"
  ]
}
